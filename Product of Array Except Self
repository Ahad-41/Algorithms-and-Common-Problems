vector<int> productExceptSelf(vector<int>& nums) {
        int sz = nums.size();
        vector <int> ans(sz);
        
        int p = 1;
        for(int i = 0; i < sz; i++) {
            ans[i] = p;
            p *= nums[i];
        }
        
        p = 1;
        for(int i = sz-1; i >= 0; i--) {
            ans[i] *= p;
            p *= nums[i];
        }
        
        return ans;
}
